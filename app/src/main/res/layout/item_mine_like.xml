<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/item_mine_like_cardview"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:clickable="true"
    android:layout_margin="@dimen/x3"
    android:foreground="?android:attr/selectableItemBackground"
    app:cardCornerRadius="@dimen/x5"
    app:cardElevation="@dimen/x3"
    app:cardPreventCornerOverlap="false"
    app:cardUseCompatPadding="true"
    app:contentPadding="0px">

    <com.dong.beautifulgirl.widget.RoundImageView
        android:id="@+id/item_mine_like_iamgeview"
        android:layout_width="@dimen/x120"
        android:layout_height="@dimen/y60"
        android:scaleType="centerCrop" />
</android.support.v7.widget.CardView>
